{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "minProperties": 1,
  "properties": {
    "cartDetails": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "quantity": {
            "type": "number",
            "minimum": 1
          },
          "size": {
            "type": "string"
          }
        },
        "required": ["quantity"],
        "additionalProperties": false
      }
    }
  },
  "required": ["cartDetails"],
  "additionalProperties": false
}
