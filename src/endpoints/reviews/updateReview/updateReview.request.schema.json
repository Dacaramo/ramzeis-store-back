{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "minProperties": 1,
  "properties": {
    "reviewTitle": {
      "type": "string",
      "maxLength": 200
    },
    "reviewDescription": {
      "type": "string",
      "maxLength": 2000
    },
    "reviewScore": {
      "type": "number",
      "minimum": 1,
      "maximum": 5
    },
    "reviewImages": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minimum": 1
    }
  }
}
